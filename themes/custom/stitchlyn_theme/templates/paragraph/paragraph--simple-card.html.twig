<section id="contact-info" class="contact-info-section py-5 bg-light" data-aos="fade-up">
  <div class="container">
    <div class="row justify-content-center text-center mb-4">
      {% for key, item in content.field_card_item %}
        {% if key|first != '#' %}
          {{ item }}
        {% endif %}
      {% endfor %}
    </div>
  </div>
</section>